import javax.swing.JOptionPane;

import conexaoBD.Conexao;
import dao.VideoDAO;
import bo.VideoBO;
import vo.Video;
import vo.Olimpiada;

public class VideoCONTROLLER {
	
	public void cadastrar(String titulo, int upc, String produtor, java.util.Date dataPublicacao, String emailProf,
			File arquivoVideo, Olimpiada olimp) {
		VideoBO bo = new VideoBO();
		bo.cadastrar(titulo,upc, produtor, dataPublicacao, emailProf, arquivoVideo, olimp);
	}
	
	public boolean consultar(int upc) {
		VideoBO bo = new VideoBO();
		return bo.consultar(upc);
	}
}
