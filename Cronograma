package vo;

import java.time.LocalDate;
import java.util.Date;

public class Cronograma {
	private String nomeOlimpiada;
	private LocalDate comecoInscricoes;
	private LocalDate fimInscricoes;
	private LocalDate comecoFase1;
	private LocalDate fimFase1;
	private LocalDate comecoFase2;
	private LocalDate fimFase2;
	private LocalDate comecoFase3;
	private LocalDate fimFase3;
	private LocalDate comecoFase4;
	private LocalDate fimFase4;
	private LocalDate comecoPremiacoes;
	private LocalDate fimPremiacoes;

	public Cronograma(String nomeOlimpiada, LocalDate comecoInscricoes, LocalDate fimInscricoes, LocalDate comecoFase1,
			LocalDate fimFase1, LocalDate comecoFase2, LocalDate fimFase2, LocalDate comecoFase3, LocalDate fimFase3,
			LocalDate comecoFase4,
			LocalDate fimFase4, LocalDate comecoPremiacoes, LocalDate fimPremiacoes) {
		
		setNomeOlimpiada(nomeOlimpiada);
		setComecoInscricoes(comecoInscricoes);
		setFimInscricoes(fimInscricoes);
		setComecoFase1(comecoFase1);
		setFimFase1(fimFase1);
		setComecoFase2(comecoFase2);
		setFimFase2(fimFase2);
		setComecoFase3(comecoFase3);
		setFimFase3(fimFase3);
		setComecoFase4(comecoFase4);
		setFimFase4(fimFase4);
		setComecoPremiacoes(comecoPremiacoes);
		setFimPremiacoes(fimPremiacoes);
	}

	public String getNomeOlimpiada() {
		return nomeOlimpiada;
	}

	public void setNomeOlimpiada(String nomeOlimpiada) {
		this.nomeOlimpiada = nomeOlimpiada;
	}

	public LocalDate getComecoInscricoes() {
		return comecoInscricoes;
	}

	public void setComecoInscricoes(LocalDate comecoInscricoes) {
		this.comecoInscricoes = comecoInscricoes;
	}

	public LocalDate getFimInscricoes() {
		return fimInscricoes;
	}

	public void setFimInscricoes(LocalDate fimInscricoes) {
		this.fimInscricoes = fimInscricoes;
	}

	public LocalDate getComecoFase1() {
		return comecoFase1;
	}

	public void setComecoFase1(LocalDate comecoFase1) {
		this.comecoFase1 = comecoFase1;
	}

	public LocalDate getFimFase1() {
		return fimFase1;
	}

	public void setFimFase1(LocalDate fimFase1) {
		this.fimFase1 = fimFase1;
	}

	public LocalDate getComecoFase2() {
		return comecoFase2;
	}

	public void setComecoFase2(LocalDate comecoFase2) {
		this.comecoFase2 = comecoFase2;
	}

	public LocalDate getFimFase2() {
		return fimFase2;
	}

	public void setFimFase2(LocalDate fimFase2) {
		this.fimFase2 = fimFase2;
	}

	public LocalDate getComecoFase3() {
		return comecoFase3;
	}

	public void setComecoFase3(LocalDate comecoFase3) {
		this.comecoFase3 = comecoFase3;
	}

	public LocalDate getFimFase3() {
		return fimFase3;
	}

	public void setFimFase3(LocalDate fimFase3) {
		this.fimFase3 = fimFase3;
	}

	public LocalDate getComecoFase4() {
		return comecoFase4;
	}

	public void setComecoFase4(LocalDate comecoFase4) {
		this.comecoFase4 = comecoFase4;
	}

	public LocalDate getFimFase4() {
		return fimFase4;
	}

	public void setFimFase4(LocalDate fimFase4) {
		this.fimFase4 = fimFase4;
	}

	public LocalDate getComecoPremiacoes() {
		return comecoPremiacoes;
	}

	public void setComecoPremiacoes(LocalDate comecoPremiacoes) {
		this.comecoPremiacoes = comecoPremiacoes;
	}

	public LocalDate getFimPremiacoes() {
		return fimPremiacoes;
	}

	public void setFimPremiacoes(LocalDate fimPremiacoes) {
		this.fimPremiacoes = fimPremiacoes;
	}
	
	
}

